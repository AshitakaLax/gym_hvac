{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File (Integrated Terminal)",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Openai Gym (Integrated Terminal)",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/tensor_force_gym_hvac.py",
			"args": [
				"Hvac-v0", 
				"-a", "examples/configs/dqn.json", 
				"-n", "examples/configs/hvac_test_network.json", 
				"-e", "1000", 
				"-D",
				"-m", "2880"],
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Full Day",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/tensor_force_gym_hvac.py",
			"args": [
				"Hvac-v0", 
				"-a", "examples/configs/ppo.json", 
				"-n", "examples/configs/hvac_test_network.json", 
				"-e", "100", 
				"-D",
				"-m", "2880"],
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Attach",
			"type": "python",
			"request": "attach",
			"port": 5678,
			"host": "localhost"
		},
		{
			"name": "Python: Module",
			"type": "python",
			"request": "launch",
			"module": "enter-your-module-name-here",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Current File (External Terminal)",
			"type": "python",
			"request": "launch",
			"program": "${file} Hvac-v0 -a examples/configs/vpg.json -n examples/configs/mlp2_network.json -e 2000 -m 200",
			"console": "externalTerminal"
		}
	]
}