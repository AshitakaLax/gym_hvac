{
    "type": "vpg_agent",
    "memory": {
        "type": "replay",
        "capacity": 5000,
        "include_next_states": false
	},
    "update_mode": {
        "unit": "episodes",
        "batch_size": 32,
        "frequency": 5
	},
	"optimizer": {
        "type":"adam",
        "learning_rate":2
    },    
    "entropy_regularization": 0.1

}
